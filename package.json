{
  "private": true,
  "type": "module",
  "name": "@feng3d/chuantou",
  "version": "0.0.11",
  "description": "穿透 - 内网穿透转发系统 | Claude Code Skills",
  "keywords": [
    "chuantou",
    "穿透",
    "tunnel",
    "proxy",
    "nat",
    "内网穿透",
    "端口转发",
    "claude-code",
    "skills",
    "agent",
    "ai"
  ],
  "workspaces": [
    "packages/*",
    "labs/*"
  ],
  "files": [
    "SKILL.md"
  ],
  "scripts": {
    "install:all": "npm install",
    "start:server": "npm run start --workspace=packages/server",
    "start:client": "npm run start --workspace=packages/client",
    "build": "npm run build --workspaces",
    "watch": "concurrently -n shared,client,server -c blue,green,yellow \"tsc --watch --pretty -p packages/shared\" \"tsc --watch --pretty -p packages/client\" \"tsc --watch --pretty -p packages/server\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "version": "node scripts/set-version.js",
    "clean": "rm -rf node_modules packages/shared/node_modules packages/client/node_modules packages/server/node_modules packages/shared/dist packages/client/dist packages/server/dist"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/feng3d-labs/chuantou.git"
  },
  "homepage": "https://github.com/feng3d-labs/chuantou",
  "bugs": {
    "url": "https://github.com/feng3d-labs/chuantou/issues"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
