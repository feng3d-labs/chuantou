{
  "name": "@feng3d/chuantou",
  "version": "0.0.1",
  "private": true,
  "description": "穿透 - 内网穿透转发系统",
  "workspaces": [
    "shared",
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install",
    "start:server": "npm run start --workspace=server",
    "start:client": "npm run start --workspace=client",
    "build": "npm run build --workspaces",
    "watch": "npm run build && npm run watch --workspaces",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "publish:all": "node scripts/publish-all.js",
    "publish:dry": "node scripts/publish-all.js --dry-run",
    "version": "node scripts/set-version.js",
    "clean": "rm -rf node_modules shared/node_modules client/node_modules server/node_modules shared/dist client/dist server/dist"
  },
  "keywords": [
    "proxy",
    "tunnel",
    "nat",
    "内网穿透",
    "端口转发"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "vitest": "^2.1.8"
  }
}
