{
  "type": "module",
  "name": "@feng3d/chuantou",
  "version": "0.0.5",
  "description": "穿透 - 内网穿透转发系统 | Claude Code Skills",
  "keywords": [
    "chuantou",
    "穿透",
    "tunnel",
    "proxy",
    "nat",
    "内网穿透",
    "端口转发",
    "claude-code",
    "skills",
    "agent",
    "ai"
  ],
  "workspaces": [
    "shared",
    "client",
    "server"
  ],
  "files": [
    "SKILL.md"
  ],
  "scripts": {
    "install:all": "npm install",
    "start:server": "npm run start --workspace=server",
    "start:client": "npm run start --workspace=client",
    "build": "npm run build --workspaces",
    "watch": "npm run build && npm run watch --workspaces",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "version": "node scripts/set-version.js",
    "clean": "rm -rf node_modules shared/node_modules client/node_modules server/node_modules shared/dist client/dist server/dist"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/feng3d-labs/chuantou.git"
  },
  "homepage": "https://github.com/feng3d-labs/chuantou",
  "bugs": {
    "url": "https://github.com/feng3d-labs/chuantou/issues"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "vitest": "^2.1.8"
  }
}
